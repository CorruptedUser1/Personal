-- Scripts:

local function TCUL_fake_script() -- handlerV1.userInfo 
	local script = Instance.new('LocalScript', handlerV1)

	script.Parent.Parent.UI:WaitForChild('userHolder').ImageLabel.TextLabel.Text = game.Players.LocalPlayer.Name
	
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local userId = player.UserId
	local thumbType = Enum.ThumbnailType.HeadShot
	local thumbSize = Enum.ThumbnailSize.Size420x420
	local content, isReady = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
	local imageLabel = script.Parent.Parent.UI:WaitForChild('userHolder').ImageLabel
	imageLabel.Image = content
	
	script.Parent.Parent.UI:WaitForChild('mainHolder').ScrollingFrame.homeFrame.TextLabel.Text = 'Welcome, '..game.Players.LocalPlayer.Name
end
coroutine.wrap(TCUL_fake_script)()
local function GNWFNU_fake_script() -- handlerV1.uiControl 
	local script = Instance.new('LocalScript', handlerV1)

	p = game.Players.LocalPlayer
	mouse = p:GetMouse()
	
	mouse.KeyDown:connect(function(key)
		if key == "n" then
			if script.Parent.Parent.UI.Visible == true then
				script.Parent.Parent.UI.Visible = false
			else
				script.Parent.Parent.UI.Visible = true
			end
		end
	end)
	
	local UserInputService = game:GetService("UserInputService")
	local runService = (game:GetService("RunService"));
	
	local gui = script.Parent.Parent.UI
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	function Lerp(a, b, m)
		return a + (b - a) * m
	end;
	
	local lastMousePos
	local lastGoalPos
	local DRAG_SPEED = (20); -- // The speed of the UI darg.
	function Update(dt)
		if not (startPos) then return end;
		if not (dragging) and (lastGoalPos) then
			gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, lastGoalPos.X.Offset, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, lastGoalPos.Y.Offset, dt * DRAG_SPEED))
			return 
		end;
	
		local delta = (lastMousePos - UserInputService:GetMouseLocation())
		local xGoal = (startPos.X.Offset - delta.X);
		local yGoal = (startPos.Y.Offset - delta.Y);
		lastGoalPos = UDim2.new(startPos.X.Scale, xGoal, startPos.Y.Scale, yGoal)
		gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, xGoal, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, yGoal, dt * DRAG_SPEED))
	end;
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			lastMousePos = UserInputService:GetMouseLocation()
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	runService.Heartbeat:Connect(Update)
	
	script.Parent.Parent.UI.sideHolder:WaitForChild('home').MouseButton1Click:Connect(function()
		local Info = TweenInfo.new(0.1)
		local tween = game:GetService('TweenService'):Create(script.Parent.Parent.UI.mainHolder.ScrollingFrame,Info,{CanvasPosition = Vector2.new(0,0)})
		tween:Play()
	end)
	
	script.Parent.Parent.UI.sideHolder:WaitForChild('localP').MouseButton1Click:Connect(function()
		local Info = TweenInfo.new(0.1)
		local tween = game:GetService('TweenService'):Create(script.Parent.Parent.UI.mainHolder.ScrollingFrame,Info,{CanvasPosition = Vector2.new(0,300)})
		tween:Play()
	end)
	
	script.Parent.Parent.UI.sideHolder:WaitForChild('players').MouseButton1Click:Connect(function()
		local Info = TweenInfo.new(0.1)
		local tween = game:GetService('TweenService'):Create(script.Parent.Parent.UI.mainHolder.ScrollingFrame,Info,{CanvasPosition = Vector2.new(0,600)})
		tween:Play()
	end)
	
	script.Parent.Parent.UI.sideHolder:WaitForChild('scripts').MouseButton1Click:Connect(function()
		local Info = TweenInfo.new(0.1)
		local tween = game:GetService('TweenService'):Create(script.Parent.Parent.UI.mainHolder.ScrollingFrame,Info,{CanvasPosition = Vector2.new(0,900)})
		tween:Play()
	end)
	
	script.Parent.Parent.UI.sideHolder:WaitForChild('settings').MouseButton1Click:Connect(function()
		local Info = TweenInfo.new(0.1)
		local tween = game:GetService('TweenService'):Create(script.Parent.Parent.UI.mainHolder.ScrollingFrame,Info,{CanvasPosition = Vector2.new(0,1200)})
		tween:Play()
	end)
	
	for i,k in pairs(script.Parent.Parent.UI.sideHolder:GetChildren()) do
		if k:IsA('TextButton') then
			k.MouseEnter:Connect(function()
				local Info = TweenInfo.new(0.1)
				local tween = game:GetService('TweenService'):Create(k,Info,{BackgroundColor3 = Color3.fromRGB(50,50,50)})
				tween:Play()
			end)
	
			k.MouseLeave:Connect(function()
				local Info = TweenInfo.new(0.1)
				local tween = game:GetService('TweenService'):Create(k,Info,{BackgroundColor3 = Color3.fromRGB(60,60,60)})
				tween:Play()
			end)
		end
	end
	
	for i,l in pairs(script.Parent.Parent.UI.sideHolder:GetChildren()) do
		if l:IsA('TextButton') then
			for i,d in pairs(l:GetChildren()) do
				if d:IsA('TextLabel') then
					d.Parent.MouseEnter:Connect(function()
						local Info = TweenInfo.new(0.1)
						local tween = game:GetService('TweenService'):Create(d,Info,{Size = UDim2.new(0, -93,0, 39)})
						tween:Play()
						d.TextTransparency = 0
					end)
	
					d.Parent.MouseLeave:Connect(function()
						d.TextTransparency = 1
						local Info = TweenInfo.new(0.1)
						local tween = game:GetService('TweenService'):Create(d,Info,{Size = UDim2.new(0, 0,0, 39)})
						tween:Play()
						d.TextTransparency = 1
					end)
				end
			end
		end
	end
end
coroutine.wrap(GNWFNU_fake_script)()
local function MAVLT_fake_script() -- handlerV1.strokeHandlerV2 
	local script = Instance.new('LocalScript', handlerV1)

	for i,v in pairs(script.Parent.Parent:GetChildren()) do
		if v:IsA('Frame') and v.Name == 'UI' then
			local stroke = Instance.new('UIStroke')
			stroke.Parent = script
			local clone = stroke:Clone()
			clone.Parent = v
			clone.Color = Color3.fromRGB(80, 80, 80)
			clone.Thickness = 2
			clone.ApplyStrokeMode = 'Border'
		end
	end
	
	for i,b in pairs(script.Parent.Parent.UI:GetChildren()) do
		if b:IsA('Frame') or b:IsA('TextButton') then
			stroke = script.UIStroke
			local clone = stroke:Clone()
			clone.Parent = b
			clone.Color = Color3.fromRGB(60, 60, 60)
			clone.Thickness = 1
			clone.ApplyStrokeMode = 'Border'
		end
	end
	
	for i,n in pairs(script.Parent.Parent.UI.sideHolder:GetChildren()) do
		if n:IsA('TextButton') then
			stroke = script.UIStroke
			local clone = stroke:Clone()
			clone.Parent = n
			clone.Color = Color3.fromRGB(81, 81, 81)
			clone.Thickness = 1
			clone.ApplyStrokeMode = 'Border'
		end
	end
	
	for i,m in pairs(script.Parent.Parent.UI.mainHolder.ScrollingFrame:GetChildren()) do
		if m:IsA('Frame') then
			for i,c in pairs(m:GetChildren()) do
				if c:IsA('Frame') then
					stroke = script.UIStroke
					local clone = stroke:Clone()
					clone.Parent = c
					clone.Color = Color3.fromRGB(81, 81, 81)
					clone.Thickness = 1
					clone.ApplyStrokeMode = 'Border'
				end
			end
		end
	end
end
coroutine.wrap(MAVLT_fake_script)()
